{
  "name": "react-healthy",
  "version": "2.0.0",
  "author": "Katia Wheeler",
  "description": "Incrementally check and alert your users to the health of your APIs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katiawheeler/healthy/issues"
  },
  "homepage": "https://github.com/katiawheeler/healthy#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/katiawheeler/healthy.git"
  },
  "keywords": [
    "react",
    "api",
    "error",
    "banner"
  ],
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "lodash.isequal": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "check-types": "tsc",
    "build": "rimraf dist && rollup -c",
    "test": "react-scripts test --watch",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint --fix -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-minify": "^0.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^28.1.3",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "react-scripts": "^5.0.1",
    "rollup": "^1.7.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "source-map-explorer": "^1.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "src/pages/**/*.{js,jsx,ts,tsx}",
      "src/hooks/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/components/index.ts"
    ]
  }
}
