{
  "name": "react-healthy",
  "version": "2.0.0",
  "description": "Incrementally check and alert your users to the health of your APIs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/katiawheeler/healthy.git"
  },
  "author": "Katia Wheeler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katiawheeler/healthy/issues"
  },
  "homepage": "https://github.com/katiawheeler/healthy#readme",
  "keywords": [
    "react",
    "api",
    "error",
    "banner"
  ],
  "scripts": {
    "build:main": "kcd-scripts build --no-clean",
    "format": "kcd-scripts format",
    "typecheck": "kcd-scripts typecheck --build ",
    "prebuild": "rimraf dist",
    "build": "yarn build:main & yarn build:bundle:main & yarn build:bundle:pure",
    "build:bundle:main": "dotenv -e .bundle.main.env kcd-scripts build -- --bundle --no-clean",
    "build:bundle:pure": "dotenv -e .bundle.main.env -e .bundle.pure.env kcd-scripts build -- --bundle --no-clean",
    "test": "react-scripts test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "lib",
    "lib-esm",
    "index.d.ts",
    "!lib/tests",
    "!lib-esm/tests"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/css": "^11.10.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-minify": "^0.5.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.21.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^28.1.3",
    "kcd-scripts": "^12.2.0",
    "prettier": "^1.16.3",
    "react": "*",
    "react-dom": "*",
    "react-scripts": "^5.0.1",
    "react-use": "^17.4.0",
    "rollup": "^2.77.3",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
